{
  "environment": "develop",
  "coverage_threshold": 70,
  "strictness_level": "moderate",
  "analysis_rules": {
    "require_happy_path": true,
    "require_error_cases": true,
    "require_null_safety_tests": false,
    "require_boundary_tests": false,
    "require_edge_cases": false,
    "allow_incomplete_scenarios": true
  },
  "quality_standards": {
    "min_assertions_per_test": 1,
    "max_test_complexity": "medium",
    "naming_convention": "should_expected_when_condition",
    "require_arrange_act_assert": false
  },
  "mock_strategy": "liberal",
  "test_categories": [
    "happy_path",
    "validation_errors", 
    "dependency_failures"
  ]
}